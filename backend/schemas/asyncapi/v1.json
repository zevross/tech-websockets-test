{
  "asyncapi": "3.0.0",
  "info": {
    "title": "zrsa-ove-demo Observatory Demo",
    "version": "0.1.0"
  },
  "servers": {
    "prod": {
      "host": "localhost:8000",
      "protocol": "ws"
    }
  },
  "channels": {
    "get_state": {
      "address": "get_state",
      "messages": {
        "receive": {},
        "send": {
          "contentType": "application/json",
          "payload": {
            "type": "object"
          }
        }
      }
    },
    "reset": {
      "address": "reset",
      "messages": {
        "receive": {},
        "send": {}
      }
    },
    "start": {
      "address": "start",
      "messages": {
        "receive": {},
        "send": {}
      }
    },
    "stop": {
      "address": "stop",
      "messages": {
        "receive": {},
        "send": {}
      }
    },
    "text_update": {
      "address": "text_update",
      "messages": {
        "receive": {
          "contentType": "application/json",
          "payload": {
            "properties": {
              "text": {
                "title": "Text",
                "type": "string"
              }
            },
            "required": [
              "text"
            ],
            "title": "TextUpdatePayload",
            "type": "object"
          }
        },
        "send": {
          "contentType": "application/json",
          "payload": {
            "properties": {
              "text": {
                "title": "Text",
                "type": "string"
              }
            },
            "required": [
              "text"
            ],
            "title": "TextUpdatePayload",
            "type": "object"
          }
        }
      }
    },
    "arc_width_update": {
      "address": "arc_width_update",
      "messages": {
        "receive": {
          "contentType": "application/json",
          "payload": {
            "properties": {
              "arc_width": {
                "title": "Arc Width",
                "type": "number"
              }
            },
            "required": [
              "arc_width"
            ],
            "title": "ArcWidthUpdatePayload",
            "type": "object"
          }
        },
        "send": {
          "contentType": "application/json",
          "payload": {
            "properties": {
              "arc_width": {
                "title": "Arc Width",
                "type": "number"
              }
            },
            "required": [
              "arc_width"
            ],
            "title": "ArcWidthUpdatePayload",
            "type": "object"
          }
        }
      }
    },
    "tick": {
      "address": "tick",
      "messages": {
        "send": {
          "contentType": "application/json",
          "payload": {
            "properties": {
              "timestamp": {
                "title": "Timestamp",
                "type": "string"
              }
            },
            "required": [
              "timestamp"
            ],
            "title": "TickPayload",
            "type": "object"
          }
        }
      }
    }
  },
  "operations": {
    "get_state.receive": {
      "action": "receive",
      "channel": {
        "$ref": "#/channels/get_state"
      },
      "messages": [
        {
          "$ref": "#/channels/get_state/messages/receive"
        }
      ]
    },
    "get_state.send": {
      "action": "send",
      "channel": {
        "$ref": "#/channels/get_state"
      },
      "messages": [
        {
          "$ref": "#/channels/get_state/messages/send"
        }
      ],
      "bindings": {
        "x-socketio": {
          "ack": true
        }
      }
    },
    "reset.receive": {
      "action": "receive",
      "channel": {
        "$ref": "#/channels/reset"
      },
      "messages": [
        {
          "$ref": "#/channels/reset/messages/receive"
        }
      ]
    },
    "reset.send": {
      "action": "send",
      "channel": {
        "$ref": "#/channels/reset"
      },
      "messages": [
        {
          "$ref": "#/channels/reset/messages/send"
        }
      ]
    },
    "start.receive": {
      "action": "receive",
      "channel": {
        "$ref": "#/channels/start"
      },
      "messages": [
        {
          "$ref": "#/channels/start/messages/receive"
        }
      ]
    },
    "start.send": {
      "action": "send",
      "channel": {
        "$ref": "#/channels/start"
      },
      "messages": [
        {
          "$ref": "#/channels/start/messages/send"
        }
      ]
    },
    "stop.receive": {
      "action": "receive",
      "channel": {
        "$ref": "#/channels/stop"
      },
      "messages": [
        {
          "$ref": "#/channels/stop/messages/receive"
        }
      ]
    },
    "stop.send": {
      "action": "send",
      "channel": {
        "$ref": "#/channels/stop"
      },
      "messages": [
        {
          "$ref": "#/channels/stop/messages/send"
        }
      ]
    },
    "text_update.receive": {
      "action": "receive",
      "channel": {
        "$ref": "#/channels/text_update"
      },
      "messages": [
        {
          "$ref": "#/channels/text_update/messages/receive"
        }
      ]
    },
    "text_update.send": {
      "action": "send",
      "channel": {
        "$ref": "#/channels/text_update"
      },
      "messages": [
        {
          "$ref": "#/channels/text_update/messages/send"
        }
      ]
    },
    "arc_width_update.receive": {
      "action": "receive",
      "channel": {
        "$ref": "#/channels/arc_width_update"
      },
      "messages": [
        {
          "$ref": "#/channels/arc_width_update/messages/receive"
        }
      ]
    },
    "arc_width_update.send": {
      "action": "send",
      "channel": {
        "$ref": "#/channels/arc_width_update"
      },
      "messages": [
        {
          "$ref": "#/channels/arc_width_update/messages/send"
        }
      ]
    },
    "tick.send": {
      "action": "send",
      "channel": {
        "$ref": "#/channels/tick"
      },
      "messages": [
        {
          "$ref": "#/channels/tick/messages/send"
        }
      ]
    }
  }
}